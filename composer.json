    "type": "library",
    "keywords": ["laravel-admin", "extension", "filter"],
    "homepage": "https://github.com/huozi1024/laravel-admin-ext-filter",
    "license": "MIT",
    "authors": [
        {
            "name": "huozi",
            "email": "lio990527@163.com",
            "homepage": "https://huozi1024.github.io"
        }
